{% if logs_stack %}
    <div class="modal_dialog" id="current-div" style="display: none; ">
        <ul class="alt" id="current">
            {% for log in logs_stack if priority >= 0 and log.priority >= priority or priority < 0 and log.context.type|default(0) == priority %}
                <li class="{{ log.priorityName }}">
                    {{ logger.display_message(loop.index, log) }}
                </li>
            {% else %}
                <li><em>No logs available for this priority.</em></li>
            {% endfor %}
        </ul>
        <img onclick="selectClose()" style="position: absolute; top: 1px; right: 5px; cursor: pointer; " width="16"
             height="16" title="" alt="close"
             src="data:image/png;base64,"/>
    </div>
{% else %}
    <p>
        <em>No logs available.</em>
    </p>
{% endif %}